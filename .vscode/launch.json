{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Server",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/server/server.mjs",
        "env": {
          "NODE_ENV": "development",
          "PORT": "8000"
        },
        "outFiles": ["${workspaceFolder}/**/*.js"]
      }
    ]
  }
  